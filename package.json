{
  "name": "hash-route",
  "version": "1.2.0",
  "description": "A router based on hash string.",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "test": "karma start karma.conf.js",
    "codecov": "codecov",
    "lib": "babel src/ -d lib/",
    "dist": "browserify -t babelify src/index.js > dist/hash-route.js",
    "dist-min": "uglifyjs dist/hash-route.js --mangle --compress > dist/hash-route.min.js",
    "prepublish": "npm run dist && npm run dist-min && npm run lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/hash-route.git"
  },
  "keywords": [
    "hash",
    "hashchange",
    "onhashchange",
    "routing",
    "route",
    "router"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kt3k/hash-route/issues"
  },
  "homepage": "https://github.com/kt3k/hash-route#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "karma": "^1.1.1",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "mocha": "^2.5.3",
    "standard": "^7.1.2",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "path-to-regexp": "^1.5.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "standard": {
    "env": [
      "browser",
      "mocha"
    ],
    "ignore": [
      "dist"
    ]
  }
}
